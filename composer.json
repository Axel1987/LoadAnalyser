{
    "name": "AxelDzhurko/LoadAnalyser",
    "description": "PHP performance tool analyser your script on time, memory usage and db query. Support Laravel and Composer for web, web console and command line interfaces.",
    "version": "1.0.0",
    "keywords": [
        "php",
        "performance",
        "performance tool",
        "php performance tool",
        "php performance analyser",
        "php-performance",
        "database",
        "query",
        "query analyser",
        "query log",
        "database query log",
        "laravel",
        "analyser",
        "compressor",
        "web",
        "terminal",
        "command line",
        "export",
        "memory",
        "time"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Axel Dzhurko",
            "homepage": "https://bitbucket.org/AlexeyDzhurko/loadanalyser.git"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "bvanhoekelen/terminal-style": "^1.0"
    },
    "require-dev": {
        "larapack/dd": "1.*",
        "phpunit/phpunit": "^5.7",
        "illuminate/database": "^5.4",
        "illuminate/events": "^5.4"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "LoadAnalyser\\": "src/"
        }
    },
    "archive": {
        "exclude": ["assets"]
    }
}
